.news-wrapper {
  padding: 20rpx 0 0;
}

.notify {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10rpx 30rpx;
  border-bottom: 1px solid #ebedf0;
}

.notify .van-image {
  margin-right: 20rpx;
}

.notify text {
  color: #323233;
  font-size: 14px;
  line-height: 24px;
}

.news-wrapper .van-button {
  height: 100%;
}